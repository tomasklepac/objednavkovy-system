{
  "name": "objednavkovy-system/app",
  "description": "Order management system with namespaces and PSR-4 autoloading",
  "type": "project",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Models\\": "app/Models/",
      "App\\Controllers\\": "app/Controllers/",
      "App\\Views\\": "app/Views/"
    }
  }
}
